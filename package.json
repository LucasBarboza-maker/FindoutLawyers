{
    "name": "@findoutlawyers.com/api",
    "version": "0.1.0",
    "description": "API for https://findoutlawyers.com website",
    "main": "dist/index.js",
    "author": {
        "email": "dax-soft@live.com",
        "name": "Michael Willian Santos"
    },
    "license": "MIT",
    "private": false,
    "scripts": {
        "start": "cross-env NODE_ENV=production node dist/index.js",
        "dev": "cross-env NODE_ENV=development yarn run nodemon --max-old-space-size=8129",
        "test": "jest",
        "build": "yarn run babel server --extensions .js,.ts --out-dir dist --copy-files --no-copy-ignored",
        "port": "node ./server/config/cli/port.js",
        "pm2:run": "pm2 start dist/index.js --name api --log logs/pm2_api.log --error logs/pm2_api.error.log --output logs/pm2_api.out.log --time --watch false",
        "pm2:clear": "pm2 delete api",
        "pm2:log": "pm2 prettylist api",
        "migrate": "yarn prisma migrate dev --preview-feature",
        "studio": "npx prisma studio -p 9999",
        "stripe": "stripe.exe",
        "stripe:webhook": "stripe listen --forward-to localhost:3001/api/client/stripe/webhook --project-name=findoutlawyers"
    },
    "dependencies": {
        "@prisma/client": "^3.0.2",
        "@vorlefan/path": "^1.7.9",
        "aws-sdk": "^2.1036.0",
        "axios": "^0.21.4",
        "bcryptjs": "^2.4.3",
        "bull-board": "^2.1.3",
        "bull-master": "^1.0.5",
        "bullmq": "^1.47.1",
        "date-fns": "^2.24.0",
        "dotenv": "^10.0.0",
        "fastify": "^3.21.3",
        "fastify-cors": "^6.0.2",
        "fastify-express": "^0.3.3",
        "fastify-helmet": "^5.3.2",
        "fastify-multer": "^2.0.2",
        "fastify-raw-body": "^3.1.0",
        "fastify-static": "^4.2.3",
        "file-type": "^16.5.3",
        "jsonwebtoken": "^8.5.1",
        "nanoid": "^3.1.25",
        "nodemailer": "^6.6.3",
        "raw-body": "^2.4.1",
        "redis": "^3.1.2",
        "socket.io": "^4.2.0",
        "stripe": "^8.197.0",
        "uuid": "^8.3.2",
        "yup": "^0.32.9"
    },
    "devDependencies": {
        "@babel/cli": "^7.15.7",
        "@babel/core": "^7.15.5",
        "@babel/node": "^7.15.4",
        "@babel/preset-env": "^7.15.6",
        "@babel/preset-typescript": "^7.15.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/date-fns": "^2.6.0",
        "@types/dotenv": "^8.2.0",
        "@types/jest": "^27.0.1",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/node": "^16.9.4",
        "@types/nodemailer": "^6.4.4",
        "@types/redis": "^2.8.32",
        "@types/yup": "^0.29.13",
        "babel-plugin-inline-react-svg": "^2.0.1",
        "babel-plugin-module-resolver": "^4.1.0",
        "chalk": "4.1.2",
        "chalk-table": "1.0.2",
        "clear": "^0.1.0",
        "cross-env": "^7.0.3",
        "jest": "^27.2.1",
        "nodemon": "^2.0.12",
        "prettier": "^2.4.1",
        "prisma": "^3.0.2",
        "rimraf": "^3.0.2",
        "ts-jest": "^27.0.5",
        "ts-node-dev": "^1.1.8",
        "tsconfig-paths": "^3.11.0",
        "typescript": "^4.4.3"
    }
}
